            {% if partners %}
            <div class="card mb-1">
              <div class="card-header">
                {{ 'Partners'|trans }}
              </div>
              <div class="card-body">
                <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                  <div class="carousel-inner">
                    {% for  key, partner in partners %}
                    {% if partner.url %}<a rel="nofollow" href="{{ partner.url }}">{% endif %}
                    <div class="carousel-item {% if key == 0 %}active {% endif %}card-img-top bg-dark p-1" data-toggle="tooltip" data-placement="top" title="{{ partner.name }}">
                      <img src="/uploads/{{ partner.image }}" class="d-block w-100" alt="{{ partner.name }}">
                    </div>
                    {% if partner.url %}</a>{% endif %}
                    {% endfor %}
                  </div>
                </div>
              </div>
            </div>
            {% endif %}
